{
  "name": "FPGAuitarFX Toolchain",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "remoteUser": "fpga",
  "runArgs": ["--platform=linux/arm64"],
  "customizations": {
    "vscode": {
      "extensions": [
        "lramseyer.vaporview",
        "surfer-project.surfer",
        "hediet.vscode-drawio",
        "analytic-signal.preview-pdf",
        "stephanvs.dot",
        "tomoki1207.pdf",
        "danielroedl.meld-diff",
        "bmpenuelas.waveform-render",
        "SimonSiefke.svg-preview"
      ]
    }
  }
}